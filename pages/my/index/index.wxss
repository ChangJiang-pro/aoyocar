page {
  width: 100%;
  background-color: #f6f6f6;
}
.nav-bar {
  position: fixed;
  width: 100%;
  top: 0;
  z-index: 999;
  background-color: #FFFFFF;
}

.back-content {
  width: 120rpx;
  border: 1px solid rgb(226, 220, 220);
  padding: 10rpx 0;
  border-radius: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.nav-bar .back {
  width: 100%;
  position: absolute;
}

.nav-bar .back .backBu {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  padding: 0 10rpx 0 20rpx;
  justify-content: space-between;
  box-sizing: border-box;
}

.nav-bar .back view {
  display: flex;
  align-items: center;
  justify-content: center;
}

.nav-bar .back image {
  width: 30rpx;
  height: 30rpx;
}
.content {
  width: 100%;
}

.photoName {
  width: 100%;
  height: 170rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  padding: 20rpx;
  box-sizing: border-box;
  justify-content: space-between;
}

.photoName .photo {
  width: 18%;
  height: 100%;
}

.photoName .photo image {
  width: 100%;
  height: 100%;
}

.photoName .name {
  width: 67%;
  height: 100%;
  padding-left: 30rpx;
  box-sizing: border-box;
}

.name view:nth-child(1) {
  width: 100%;
  height: 50%;
  display: flex;
  align-items: center;
  font-size: 26rpx;
}

.name view:nth-child(2) {
  width: 100%;
  height: 50%;
  font-size: 26rpx;
}

.photoName .setting {
  width: 10%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.photoName .setting image {
  width: 40rpx;
  height: 40rpx;
}

/* <!-- 关注  优惠券 车库 爱车 --> */
.discounts {
  width: 100%;
  height: 220rpx;
  background-color: #fff;
  display: flex;
}

.discounts .attention,
.discounts .ticket,
.discounts .carList,
.discounts .loveCar {
  width: 25%;
  height: 100%;
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.discounts .attention view:nth-child(1),
.discounts .ticket view:nth-child(1),
.discounts .carList view:nth-child(1),
.discounts .loveCar view:nth-child(1){
  margin-bottom: 15rpx;
  font-size: 30rpx;
  font-weight: 700;
}
/* 实名认证 */
.authentication{
  width: 100%;
  height: 190rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  background-color: #fff;
  margin-bottom: 30rpx;
}
.authentication image{
  width: 100%;
  height: 100%;
}
.orderForm{
  width: 100%;
  height: 300rpx;
  padding:20rpx;
  box-sizing: border-box;
}
.orderFormContent{
  width: 100%;
  height: 100%;
  background-color: #fff;
  border-radius: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
}
.lookAll{
  width: 100%;
  height: 60rpx;
  display: flex;
  padding: 0 0 0 30rpx;
  box-sizing: border-box;
  justify-content: space-between;
}
.lookAll view:nth-child(1){
  font-size: 26rpx;
  font-weight: 600;
}
.lookAll view:nth-child(2){
  font-size: 26rpx;
  color: #666666;
}
.status,.serve{
  width: 100%;
  display: flex;
}
.status .status-proceed,.status .status-accomplish,.status .status-evaluate{
  width: 25%;
  height: 100%;
  padding: 30rpx 0 20rpx 0;
  box-sizing: border-box;
  font-size: 26rpx;
  color: #666666;
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
  position: relative;
}
.status .status-proceed .number,.status .status-accomplish .number,.status .status-evaluate .number{
  width: 25rpx;
  height: 25rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 25rpx;
  border: 1px solid #940B0B;
  color: #940B0B;
  font-size: 18rpx;
  position: absolute;
  top:5rpx;
  left:110rpx;
}
.status .status-proceed image,.status .status-accomplish image,.status .status-evaluate image{
  width: 50rpx;
  height: 50rpx;
  margin-bottom: 15rpx;
}
.serve .serve-proceed,.serve .serve-accomplish,.serve .serve-evaluate{
  width: 25%;
  height: 100%;
  padding: 30rpx 0 20rpx 0;
  box-sizing: border-box;
  font-size: 26rpx;
  color: #666666;
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
}
.serve .serve-proceed image,.serve .serve-accomplish image,.serve .serve-evaluate image{
  width: 70rpx;
  height: 70rpx;
  margin-bottom: 15rpx;
}